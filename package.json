{
  "name": "gowithme-api",
  "version": "1.0.0",
  "description": "Handles api for gowithme app",
  "main": "index.js",
  "scripts": {
    "start:rest-server": "cd rest-server && npm run start",
    "setup:rest-server": "cd rest-server && npm install",
    "db:setup:rest-server": "cd rest-server && babel-node src/config/database/setup.js",
    "buildEnv": "node bin/buildEnv.js",
    "changePass": "cd rest-server && mysql -u root -e 'CREATE USER 'gowithme'@'localhost' IDENTIFIED BY 'gowithme';' && mysql -u root -e 'SET PASSWORD FOR 'gowithme'@'localhost' = PASSWORD('password');' && mysql -u root -e 'GRANT ALL ON stockmarket.* TO 'makler'@'localhost';'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GoWithMeL1FB/gowithme-api.git"
  },
  "author": "David Yonwook Chung",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GoWithMeL1FB/gowithme-api/issues"
  },
  "homepage": "https://github.com/GoWithMeL1FB/gowithme-api#readme",
  "dependencies": {
    "lodash": "^4.17.4",
    "mongoose": "^5.0.3",
    "mysql": "^2.15.0",
    "node": "^9.4.0",
    "npm-run-all": "^4.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0"
  }
}
